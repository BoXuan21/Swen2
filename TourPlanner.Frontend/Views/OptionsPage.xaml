<UserControl x:Class="TourPlanner.Frontend.Views.OptionsPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TourPlanner.Frontend.Views"
             mc:Ignorable="d" 
             Background="{DynamicResource WindowBackground}"
             d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel Margin="10" Background="#EEE">
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Height="50" Width="60" Margin="10">Import</Button>
            <Button Height="50" Width="60" Margin="10">Export</Button>
        </StackPanel>
    </DockPanel>
</UserControl>
