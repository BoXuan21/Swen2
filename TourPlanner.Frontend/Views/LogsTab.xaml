<UserControl x:Class="TourPlanner.Frontend.Views.LogsTab"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TourPlanner.Frontend.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Margin="10" Background="#CCC">
        <DataGrid>
            <DataGrid.Columns>
                <DataGridTextColumn Header="Date" />
                <DataGridTextColumn Header="Title" />
                <DataGridTextColumn Header="Difficulty" />
                <DataGridTextColumn Header="Total Distance" />
                <DataGridTextColumn Header="Total Time" />
                <DataGridTextColumn Header="Rating" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</UserControl>
